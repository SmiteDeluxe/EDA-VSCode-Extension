{
  "name": "eda-test01",
  "displayName": "EDA_Test01",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "eda-test01.runEda",
        "title": "Start",
        "category": "EDA"
      },
      {
        "command": "eda-test01.runEdaFromTable",
        "title": "Start EDA from selection",
        "category": "EDA"
      },
      {
        "command": "eda-test01.refreshWebview",
        "title": "Refresh Webview",
        "category": "EDA"
      },
      {
        "command": "eda-test01.runEdaFromContext",
        "title": "Explore Table",
        "category": "EDA"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "eda-test01.runEdaFromContext"
        }
      ]
    }
  },
  "scripts": {
    "watch:svelte": "cd svelte && vite build --watch --emptyOutDir",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch:extension": "webpack --watch",
    "watch:mini-server": "cd flask && watchmedo shell-command --patterns='*.py' --recursive --command='pyinstaller ./python-mini-server/main.py -F --hidden-import=flask --distpath=../src/compiled-mini-server | chmod +x ../src/compiled-mini-server/main'",
    "watch-all": "npm-run-all --parallel watch:*",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "glob": "^8.1.0",
    "html-loader": "^4.2.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.4.4",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}
